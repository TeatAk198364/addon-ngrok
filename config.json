{
  "name": "Ngrok",
  "version": "0.1.3",
  "slug": "ngrok",
  "url": "https://github.com/kjell5317/addon-ngrok",
  "description": "This addon creates Ngrok tunnels to your instance.",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "4040/tcp": 4040
  },
  "options": {
    "ng_token": null,
    "drache": {
      "addr": null,
      "cf_zone": null,
      "cf_token": null
    }
  },
  "schema": {
    "ng_token": "str",
    "drache": {
      "addr": "str",
      "cf_zone": "str",
      "cf_token": "str"
    }
  },
  "map": ["config"]
}
